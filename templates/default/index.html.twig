{% extends 'base.html.twig' %}

{% block body %}
    <table>
        <thead>
        <tr>
            <th>id</th>
            <th>name</th>
            <th>total</th>
        </tr>
        </thead>
        <tbody>
        {% for list in lists %}
            <tr>
                <td>{{ list.id }}</td>
                <td>{{ list.name }}</td>
                <td>{{ list.total_tasks }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
